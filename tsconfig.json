{
  "files": [
    "./main.ts",
    "./messages.ts",
    "./rpc-file-hash.ts"
  ],
  "compilerOptions": {
    "target": "es5",
    "typeRoots": [
      "./node_modules"
    ],
    "outFile": "./build/index.js",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "plugins": [
      {
        "transform": "ts-auto-mock/transformer",
        "cacheBetweenTests": false
      }
    ]
  }
}
